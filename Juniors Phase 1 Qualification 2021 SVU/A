//https://codeforces.com/group/FWyJjChFAe/contest/328360/problem/A
//alyaa999
#include <bits/stdc++.h>
using namespace std ;
int main()
{
     string s;
     cin >> s >> s ;
     string t =  s ;
 
 
    for (int i = 0; i < s.length(); ++i)
    {
        if (isalpha(s[i]))
            s[i] = ' ';
    }
     for (int i = 0; i < t.length(); ++i)
    {
        if (!isalpha(t[i]))
            t[i] = ' ';
    }
 
    stringstream ss(s);
    stringstream tt (t);
    int x;
    string m ;
 
    while (ss >> x && tt>>m)
    {
       for(int i=0;i<x;i++)
        cout << m ;
    }
 
}
